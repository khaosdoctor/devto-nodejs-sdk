  "description": "The Dev.to Node.js SDK",
  "main": "dist/index.js",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "clean": "rm -rf ./dist",
    "test": "echo 0",
    "lint": "tslint --project tsconfig.json",
    "lint:fix": "tslint --fix --project tsconfig.json",
    "build": "tsc",
    "build:debug": "npm run clean && npm run build -- --sourceMap",
