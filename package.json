  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^11.1.0",
    "@types/node": "^15.0.2",
    "@types/sinon": "^10.0.0",
    "chai": "^4.3.4",
